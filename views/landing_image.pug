extends layout.pug

block append section 

  div.container-fluid

    div.jumbotron
      if memberhip_role === "Dreamweaver" || membership_role === 'Project Manager' || admin_role !== "Inactive" 
        div.d-flex.justify-content-center
          img(src="/assets/images/logos/animateLogoLinks.svg" alt="logo" height= 200)
        div.d-flex-column.align-items-center 
          div.d-flex.justify-content-center.mt-3
            h2.display Special Projects 
          div.d-flex.justify-content-center.mt-3
            h3.display-5
              a(href="/phoenicia/" + id) Phoenicia 
              br 
              a(href="/transcendence/" + id) Transcendence 
      else 
        div.d-flex-column.align-items-center
          div.d-flex.justify-content-center.mt-3
            h1.display-4 Special Projects Area 
          div.d-flex.justify-content-center.mt-3
            p.lead For Deamweavers only
        
      


  